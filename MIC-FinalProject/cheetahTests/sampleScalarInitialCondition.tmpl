FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      $object;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      $dimensions;

internalField   $internalField;

boundaryField
{
#for $boundary in $boundaries
	$boundary.name
	{
		type			$boundary.type
		value			$boundary.value
	}
#end for
}


